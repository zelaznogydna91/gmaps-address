    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "ITALICIZED KEYWORDS",
                "scope": [
                    "entity.name.type.class", //class names
                    "entity.name.type.js", // new … Expression
                    "entity.other.attribute-name.jsx",
                    "keyword.control.conditional.js", // if
                    "keyword.control.flow.js", // await
                    "keyword.control.from.js", // From-Keyword
                    "keyword.control.import.js", // Imports
                    "keyword.control.loop.js", // for
                    "keyword.operator.new.js", // new
                    "storage.modifier", //static keyword
                    "storage.type.class.js", //class keyword
                    "storage.type.extends.js", // extends keyword
                    "storage.type.function.js", // function keyword
                    "storage.type.js", // Variable declarations
                    "support.class.builtin.js", //String, Number, Boolean…, this, super
                ],
                "settings": {
                    "fontStyle": "italic",
                },
            },
            {
                "name": "EXCLUDE FROM ITALICS",
                "scope": [
                    "invalid",
                    "keyword.operator",
                    "constant.numeric.css",
                    "keyword.other.unit.px.css",
                    "constant.numeric.decimal.js",
                    "constant.numeric.json",
                    "entity.name.type.class.js",
                ],
                "settings": {
                    "fontStyle": "",
                },
            },
            {
                "name": "PUNCTUATION",
                "scope": [
                    "keyword.operator.accessor.js", // The dot of [Class].[Property]
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                },
            },
            {
                "name": "OBJECT_KEYS",
                "scope": [
                    "string.unquoted.js",
                ],
                "settings": {
                    "foreground": "#ff00ae",
                    "fontStyle": ""
                },
            },
            {
                "name": "STRING",
                "scope": [
                    "string.quoted.single.js",
                ],
                "settings": {
                    "foreground": "#3b79ff",
                    "fontStyle": ""
                },
            },
            {
                "name": "JSON_PROPERTY_NAME",
                "scope": [
                    "support.type.property-name.json.comments",
                ],
                "settings": {
                    "foreground": "#86ffd58a",
                    "fontStyle": ""
                },
            },
            {
                "name": "JSON_STRING_DOUBLE_QUOTE",
                "scope": [
                    "string.quoted.double.json.comments",
                ],
                "settings": {
                    "foreground": "#ff84c4",
                    "fontStyle": ""
                },
            },
            {
                "name": "JSON_PROPERTY_NAME",
                "scope": [
                    "constant.language.json.comments",
                ],
                "settings": {
                    "foreground": "#00a6ff",
                    "fontStyle": ""
                },
            },
            {
                "name": "JSON_PROPERTY_NAME",
                "scope": [
                    "constant.numeric.json.comments",
                ],
                "settings": {
                    "foreground": "#ff7300",
                    "fontStyle": "",
                },
            },
            {
                "name": "MODULE_IMPORT_FROM",
                "scope": [
                    "keyword.control.module.js",
                ],
                "settings": {
                    "foreground": "#ff7300",
                    "fontStyle": "",
                },
            },
            {
                "name": "MODULE_STRINGS",
                "scope": [
                    "string.quoted.module.js",
                    "punctuation.definition.string.begin.js"
                ],
                "settings": {
                    "foreground": "#5AD157",
                    "fontStyle": "",
                },
            },
        ]
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.colorTheme": "Cobalt2",
    "window.zoomLevel": 1.5,
